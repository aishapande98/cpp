using namespace std;

int main() {
	int t,n,i;
	cin>>t;
	while(t--)
	{
	    cin>>n;
	    int flag =0;
	    for(i=1;i<n;i++)
	    {
	        if(n==(i*i))
	        {
	            flag=1;
	            break;
	        }
	        if(i*i>n)
	        {
	            break;
	        }
	        
	    }
	    if(flag)
	    cout<<"1"<<endl;
	    else
	    cout<<"0"<<endl;
	}
	return 0;
}
