#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n,i;
    cin>>n;
    n=n+1;
    int f[20];
   
    for(i=1;i<n;i++)
    {
        cin>>f[i];
    }
    for(i=1;i<n;i++)
    {
        if(f[f[i]]!=i)
        {
            cout<<"NO";
            goto x;
        }
        
    }
     cout<<"YES";
    x:
    return 0;
}
