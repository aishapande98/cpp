#include <iostream>
using namespace std;

int main() {
    int t,no;
    cin>>t;
    for(int i=0;i<t;i++){
        cin>>no;
	int fib1 = 0, fib2 = 1, fib3 = 1;
 
    cout << fib1 << " " << fib2 << " ";
    while (fib1 + fib2 <= no)
    {
        fib3 = fib1 + fib2;
        fib1 = fib2;
        fib2 = fib3;
        cout << fib3 << " ";
    }
    cout << endl;
    }
	return 0;
}
